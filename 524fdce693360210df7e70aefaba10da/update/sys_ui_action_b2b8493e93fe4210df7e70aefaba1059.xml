        <name>Deploy Asset/Agent</name>
        <onclick>createTask()</onclick>
        <order>100</order>
        <script><![CDATA[current.update();

var gr = new GlideRecord("x_1387186_ows_satellite_task");
gr.initialize();
gr.setValue('satellite', current.satellite);
gr.setValue('asset_agent', current.agent_asset);
gr.setValue('location', current.satellite.location);
gr.setValue('assignment', current.sys_id);
if(current.table=="x_1387186_aios_agent"){
			gr.assigned_to = current.asset_agent;
		}
		else{
			var grAgent = new GlideRecord("x_1387186_aios_agent");
			grAgent.addQuery("agent_id",current.managing_agent);
			grAgent.query();
			
			if(grAgent.next()){
				
				gr.assigned_to = grAgent.sys_id;
			}
		}

gr.insert();

gs.addInfoMessage("New Satellite Task Created with number: "+gr.number);

action.setRedirectURL(gr);

]]></script>
